{
	"name": "covalent-cloud-webapp",
	"version": "1.0.0",
	"description": "SaaS variant of the Covalent web app to be deployed onto Agnostiqâ€™s AWS cloud infrastructure and used by B2C users",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"lint": "eslint ./",
		"lint-fix": "eslint ./ --fix",
		"format": "prettier --write \"**/*.{js,jsx,json,md}\"",
		"prod": "rimraf build",
		"dev": "rimraf build",
		"prodBuild": "env-cmd -f .env.production  yarn run prod && webpack --config=./config/webpack.prod.js",
		"devBuild": "env-cmd -f .env.development  yarn run dev && webpack --config=./config/webpack.dev.js",
		"start": "env-cmd -f .env.local  webpack-dev-server --history-api-fallback --config=./config/webpack.config.js --open",
		"prepare": "husky install"
	},
	"keywords": [],
	"author": "PsiogDigtal",
	"license": "ISC",
	"dependencies": {
		"@emotion/react": "^11.8.2",
		"@emotion/styled": "^11.8.1",
		"@mui/icons-material": "^5.5.1",
		"@mui/lab": "^5.0.0-alpha.74",
		"@mui/material": "^5.5.1",
		"@mui/styles": "^5.5.1",
		"axios": "^0.26.1",
		"classnames": "^2.3.1",
		"moment": "^2.29.1",
		"prop-types": "^15.8.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router-dom": "^6.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.17.5",
