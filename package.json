		"babel-jest": "^27.5.1",
		"babel-loader": "^8.2.3",
		"css-loader": "^6.7.1",
		"dotenv-webpack": "^7.1.0",
		"env-cmd": "^10.1.0",
		"eslint": "^8.10.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-react-app": "^7.0.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
