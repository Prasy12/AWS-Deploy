		"mini-css-extract-plugin": "^2.6.0",
		"prettier": "^2.5.1",
		"react-test-renderer": "^17.0.2",
		"rimraf": "^3.0.2",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"url-loader": "^4.1.1",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.7.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
